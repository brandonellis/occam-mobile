# Stripe publishable key (pk_test_... or pk_live_...)
# Get this from your Stripe dashboard or the backend's .env STRIPE_KEY value
EXPO_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_key_here

# Google OAuth web client ID (required for native Google Sign-In)
# This is the WEB client ID from Google Cloud Console (same as GOOGLE_CLIENT_ID in backend .env)
# The native SDK uses the web client ID to obtain an idToken that the backend can verify
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=your_google_web_client_id_here

# Google OAuth iOS client ID (required for native Google Sign-In on iOS)
# This is the iOS client ID from Google Cloud Console â€” its reversed client ID must also
# be added as a CFBundleURLScheme in app.json under ios.infoPlist.CFBundleURLTypes
EXPO_PUBLIC_IOS_CLIENT_ID=your_google_ios_client_id_here
